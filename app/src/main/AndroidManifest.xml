        <activity android:name="com.example.kotlin.normal.logic.LogicActivity" />
        <activity android:name="com.example.kotlin.normal.obj.ObjActivity" />

        <activity android:name="com.example.kotlin.jetpack.JetpackActivity" />
        <activity android:name="com.example.kotlin.jetpack.lifecycle.LifeCycleActivity" />
        <activity android:name="com.example.kotlin.jetpack.viewmodel.ViewModelActivity" />
        <activity android:name="com.example.kotlin.jetpack.viewmodel.ViewModelActivity2" />
        <activity android:name="com.example.kotlin.jetpack.livedata.LiveDataActivity" />

